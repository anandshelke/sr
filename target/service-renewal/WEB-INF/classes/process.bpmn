      <bpmn:outgoing>Flow_1hd2uyu</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Activity_1y05276" name="Recurring credit card payment from Comviva" camunda:class="com.TechM.poc.service_renewal.CreditCardPaymentDelegate">
